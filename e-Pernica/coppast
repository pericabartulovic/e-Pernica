function dodaj_nulti($predmet1, $predmet2, $predmet3, $predmet4, $predmet5, $korisnik, $konekcija){
    $sql = "INSERT INTO raspored VALUES (null, ?, ?, ?, ?, ?, ?)";
    $upit = $konekcija->prepare($sql);
    return $upit->execute([$predmet1, $predmet2, $predmet3, $predmet4, $predmet5, $korisnik]);
}

function dodaj_prvi($predmet1, $predmet2, $predmet3, $predmet4, $predmet5, $korisnik, $konekcija){
